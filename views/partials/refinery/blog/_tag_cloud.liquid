<div id="tag_cloud" class="tab-pane fade in active" style="text-align: center;">
    {% capture_variable tag_cloud %}{% blog_tag_cloud %}{% endcapture_variable %}
    {% for tag in tag_cloud %}
    <a href="{{ tag.url }}" class="{{ tag.css_class }} label label-info">&nbsp;{{ tag.name }}</a>
    {% endfor %}
</div>